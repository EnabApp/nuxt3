{
    "rewrites": [
        {
            "source": "/(.*)",
            "has": [
                {
                    "type": "host",
                    "value": "*.enab.app"
                }
            ],
            "destination": "/domain/"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "buildCommand": "yarn build",
    "devCommand": "yarn dev",
    "installCommand": "yarn install"
}