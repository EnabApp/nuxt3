{
    "rewrites": [
        {
            "source": "/:path*",
            "has": [
              {
                "type": "host",
                "value": "test3.enab.app"
              }
            ],
            "destination": "/domain/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "buildCommand": "yarn build",
    "devCommand": "yarn dev",
    "installCommand": "yarn install"
}