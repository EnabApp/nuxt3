{
    "rewrites": [
        {
            "source": "/(.*)",
            "has": [
                {
                    "type": "host",
                    "value": "(?<paramName>.*)"
                }
            ],
            "destination": "/domain/:paramName"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "buildCommand": "yarn build",
    "devCommand": "yarn dev",
    "installCommand": "yarn install"
}