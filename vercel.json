{
    "rewrites": [
        {
            "source": "/:path*",
            "has": [
                {
                    "type": "host",
                    "value": "*.enab.app"
                }
            ],
            "destination": "/domain/:path*"
        }
    ],

    "buildCommand": "yarn build",
    "devCommand": "yarn dev",
    "installCommand": "yarn install"
}